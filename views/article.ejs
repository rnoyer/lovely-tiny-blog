<%- include('./partials/header.ejs') %>
<article class="article">
  <div class="article_top">
    <h1 class="custom_h1"><%= data.title %></h1>
    <nav>
      <button onclick="window.location='/'" class="post_button" >ğŸ </button>
    </nav>
  </div>
  <div class="article_content">
    <% for( paragraph of data.content) { %>
      <p class="custom_p"><%= paragraph %></p>
